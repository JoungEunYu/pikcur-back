<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pikcurchu.pikcur.mapper.AlarmMapper">

    <select id="selectAlarmList" resultType="com.pikcurchu.pikcur.dto.response.ResAlarmDto">
        SELECT
            *
        FROM
            alarm
        WHERE
            member_no = #{memberNo}
    </select>
</mapper>